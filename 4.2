function reverseArray(array) {
let newArray = [];
  for (i = array.length - 1; i >= 0; i--) {
  	newArray.push(array[i]);
  }
  return newArray;
}

function reverseArrayInPlace(array) {
	for (i = array.length - 1; i >= 0; i--) {
    array.push(array[i]);
    array.splice(i, 1);
    }
  return array;
}

